DEFINE QLOCAL (DEV.Q.TEST.IN)
DEFINE QLOCAL (DEV.Q.TEST.OUT)
DEFINE QLOCAL (Q.TEST.IN)
DEFINE QLOCAL (Q.TEST.OUT)

SET AUTHREC PROFILE(Q.TEST.IN) OBJTYPE(QUEUE) +
PRINCIPAL('app') AUTHADD(PUT, GET, INQ, BROWSE)

SET AUTHREC PROFILE(Q.TEST.OUT) OBJTYPE(QUEUE) +
PRINCIPAL('app') AUTHADD(PUT, GET, INQ, BROWSE)
